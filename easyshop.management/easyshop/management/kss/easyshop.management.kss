a.move_category_up:click {
    evt-click-preventdefault: True;    
    action-server: kss_move_category_up;
    kss_move_category_up-uid: kssAttr("uid");
}

a.move_category_down:click {
    evt-click-preventdefault: True;    
    action-server: kss_move_category_down;
    kss_move_category_down-uid: kssAttr("uid");
}