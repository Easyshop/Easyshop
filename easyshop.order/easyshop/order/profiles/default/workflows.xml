<?xml version="1.0"?>
<object name="portal_workflow" meta_type="Plone Workflow Tool">
 <property
    name="title">Contains workflow definitions for your portal</property>
 <object name="easyshop_order_workflow" meta_type="Workflow"/>
 <object name="easyshop_product_workflow" meta_type="Workflow"/>
 <bindings>
  <type type_id="EasyShop">
   <bound-workflow workflow_id="easyshop_product_workflow"/>        
  </type>   
  <type type_id="Category">
   <bound-workflow workflow_id="easyshop_product_workflow"/>
  </type>
  <type type_id="Order">
   <bound-workflow workflow_id="easyshop_order_workflow"/>
  </type>
  <type type_id="Product">
   <bound-workflow workflow_id="easyshop_product_workflow"/>
  </type>
 </bindings>
</object>